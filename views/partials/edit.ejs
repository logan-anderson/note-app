<div class="col col-edit form-group">
    <div class="row py-3">
        <label for="title" class="col-sm-2 col-form-label px-0"> Title</label>
        <div class="col-sm-10 px-0" >
            <input name="title" class="form-control" id="title-input-edit" type="text"
                value="<%= note.title %>">
            </input>
        </div>
    </div>
    <%for(let i=0;i < note.list_content.length;i++){ %>
    <div class="row list-items-row-edit">
        <div class="input-group mb-3">
            <input type="text" class="form-control" placeholder="Add Item" aria-label="Add Item"
                aria-describedby="basic-addon2" value="<%= note.list_content[i] %>">
            <div class="input-group-append">
                <button class="btn btn-outline-secondary add-item-btn-edit" type="button">Add
                    Item</button>
            </div>
        </div>
    </div>
    <% }%>
</div>